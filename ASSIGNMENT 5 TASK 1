#Create a Dictionary of Student Marks
student_marks = {
    "Alice": 85,
    "Bob": 90,
    "Charlie": 78,
    "Diana": 92
}

name = input("Enter the student's name: ").strip().title()

if name in student_marks:
    print(name + "'s marks:", student_marks[name])
else:
    print("Student not found.")
